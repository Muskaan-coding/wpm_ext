extends layout

block content
  #banner.page-header
    .row
      .col-md-12.col-sm-12
        br
        br
        center
          h1= title
  .row
    center
      .col-sm-3 
        center
          form
            .form-group
                label.form-label.mt-4(for='exampleInputEmail1') Email address
                input#exampleInputEmail1.form-control(type='email',aria-describedby='emailHelp',placeholder='Enter email',fdprocessedid='nxlxc')
                small#emailHelp.form-text.text-muted We'll never share your email with anyone else.
            .form-group
                label.form-label.mt-4(for='exampleInputPassword1') Password
                input#exampleInputPassword1.form-control(type='password',placeholder='Password',autocomplete='off',fdprocessedid='q0khqs')
            br
            br
            button.btn.btn-primary(type='submit',fdprocessedid='pk3hzt') Submit